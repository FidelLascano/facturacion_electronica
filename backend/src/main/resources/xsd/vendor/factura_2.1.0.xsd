<?xml version="1.0" encoding="UTF-8"?>
<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified">
  <xsd:element name="factura">
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element name="infoTributaria" minOccurs="1" maxOccurs="1">
          <xsd:complexType>
            <xsd:sequence>
              <xsd:element name="claveAcceso" type="xsd:string"/>
              <xsd:element name="ruc" type="xsd:string" minOccurs="0"/>
            </xsd:sequence>
          </xsd:complexType>
        </xsd:element>
        <xsd:element name="info" minOccurs="0" maxOccurs="1">
          <xsd:complexType>
            <xsd:sequence>
              <xsd:element name="secuencial" type="xsd:string" minOccurs="0"/>
              <xsd:element name="importeTotal" type="xsd:string" minOccurs="0"/>
            </xsd:sequence>
          </xsd:complexType>
        </xsd:element>
      </xsd:sequence>
      <xsd:attribute name="id" type="xsd:string" use="optional"/>
      <xsd:attribute name="version" type="xsd:string" use="required" fixed="2.1.0"/>
    </xsd:complexType>
  </xsd:element>
</xsd:schema>
